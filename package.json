{
  "name": "spangle",
  "version": "0.7.3",
  "description": "Just as a task runner",
  "repository": "https://github.com/tkamenoko/spangle",
  "author": "T.Kameyama <tkamenoko@vivaldi.net>",
  "license": "MIT",
  "scripts": {
    "doc:serve": "poetry run mkdocs serve",
    "doc:build": "poetry run mkdocs build",
    "test:unit": "poetry run green -vvv -r -U -s 1 -O -u 'spangle/**' -o '**/_*,tests/*,.venv/*' $*",
    "test:unit_all": "run test:unit tests ; run report:coverage",
    "report:coverage": "poetry run coverage html",
    "test:e2e": "run urls:gen && pnpify testyts ; run urls:rm",
    "test": "run test:unit_all && run test:e2e",
    "urls:gen": "poetry run spangle urls-ts tests_js.src.app:api > tests_js/src/urls.ts",
    "urls:rm": "rm tests_js/src/urls.ts",
    "build": "poetry build"
  },
  "private": true,
  "devDependencies": {
    "@types/got": "^9.6.11",
    "@types/node": "^13.13.12",
    "@types/typescript": "^2.0.0",
    "@types/ws": "^7.2.5",
    "@yarnpkg/pnpify": "^2.0.0-rc.25",
    "got": "^11.3.0",
    "isomorphic-ws": "^4.0.1",
    "testyts": "^1.0.1",
    "typescript": "^3.9.5",
    "ws": "^7.3.0"
  }
}
